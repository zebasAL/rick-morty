{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["$VERCEL_ENV", "^build"],
      "outputs": ["dist/", ".next/"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "web#build": { 
      "dependsOn": ["$VERCEL_ENV", "^build"],
      "outputs": [".next/**"]
    },
    "test": {
      "cache": false
    }
  }
}