{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/",
        ".next/"
      ],
      "env": [
        "VERCEL_ENV",
        "VERCEL_URL"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "web#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ],
      "env": [
        "VERCEL_ENV",
        "VERCEL_URL"
      ]
    },
    "test": {
      "cache": false
    }
  }
}
